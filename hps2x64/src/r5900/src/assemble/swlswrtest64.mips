	sdl %r1, $44(%r0)	; 40 + 4
	sdl %r1, $53(%r0)	; 48 + 5
	sdl %r1, $62(%r0)	; 56 + 6
	sdl %r1, $71(%r0)	; 64 + 7
	
	
; expected results (at this line):
