; $56 = 01 02 03 04 05 06 07 00
; $64 = 00 01 02 03 04 05 06 07

	sdr %r1, $8(%r0)	; 8 + 0
	sdr %r1, $17(%r0)	; 16 + 1
	sdr %r1, $26(%r0)	; 24 + 2
	sdr %r1, $35(%r0)	; 32 + 3
	sdr %r1, $44(%r0)	; 40 + 4
	sdr %r1, $53(%r0)	; 48 + 5
	sdr %r1, $62(%r0)	; 56 + 6
	sdr %r1, $71(%r0)	; 64 + 7
	
	sll %r0, %r0, $0
	sll %r0, %r0, $0
	sll %r0, %r0, $0
	sll %r0, %r0, $0

	break

