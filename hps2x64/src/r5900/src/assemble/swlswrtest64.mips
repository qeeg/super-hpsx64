
SWLSWRtest:

	addiu %r1, %r0, $0
	addiu %r2, %r0, $1
	addiu %r3, %r0, $2
	addiu %r4, %r0, $3
	addiu %r5, %r0, $4
	addiu %r6, %r0, $5
	addiu %r7, %r0, $6
	addiu %r8, %r0, $7
	
	sb %r1, $0(%r0)
	sb %r2, $1(%r0)
